import java.util.*;
public class Main {

	public static void main(String[] args){
		Scanner scan = new Scanner(System.in);
				
		int N = scan.nextInt();
		int result = 0;
		for(int i = 0; i <= N; i++) {
			if(i == 0) {
				result++;
			} else {
				result *= i;
			}
		}
		System.out.print(result);
		
	}

}
