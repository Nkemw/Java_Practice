import java.util.*;

public class Main {

	public static void main(String[] args){
		Scanner scan = new Scanner(System.in);
		
		String str = scan.next();
		int[] indexing = {3,3,3,4,4,4,5,5,5,6,6,6,7,7,7,8,8,8,8,9,9,9,10,10,10,10};
		int time = 0;
		
		for(int i = 0; i < str.length(); i++) {
			time += indexing[str.charAt(i)-'A'];
		}
		
		System.out.print(time);
		
		
	}
}
